                        success: function (data) {
                            if("fileUpload_succ"==data.message){
                                alert("图片上传成功！");
                                return true;
                            }else{
                                // $.messager.alert('提示', data.message);
                                alert("图片上传失败");
                            }
                        },
                        resetForm: true
                    };
            if(confirm("亲，确定提交么？")){
                $("#uploadForm").ajaxSubmit(options);
                return true;
            }else{
                return false;
            }
        });

        $("#timelinePicControlBtn").click(function(){
            var isShow = $('#timelinePicture').css('display');
            if(isShow =='block'){
                $('#timelinePicture').css('display','none');
                $("#timelinePicControlBtn").text("显示");
            }else{
                $('#timelinePicture').css('display','block');
                $("#timelinePicControlBtn").text("隐藏");
            }
        });

        $('#uploadHeadImageButton').click(function(){
            var options =
                    {
                        url: "/behavior/headImage",
                        type: 'post',
                        contentType:"multipart/form-data", //"text/xml",
                        cache:false,//cache设置为false，上传文件不需要缓存。
                        success: function (data) {
                            if("fileUpload_succ"==data.message){
                                alert("图片上传成功！");
                                return true;
                            }else{
                                // $.messager.alert('提示', data.message);
                                alert("图片上传失败");
                            }
                        },
                        resetForm: true
                    };
            if(confirm("亲，确定提交么？")){
                $("#uploadHeadImageForm").ajaxSubmit(options);
                return true;
            }else{
                return false;
            }
        });

        $('#widget-user-image').click(function(){
            var css = $("#avatarSlect").css('display');
            if(css=='none'){
                $('#avatarSlect').css('margin-left', '92px');
                $('#avatarSlect').css('margin-top', '-34px');
                $('#avatarSlect').css('display', 'block');
                $('#uploadHeadImageButton').css('margin-right', '160px');
                $("#uploadHeadImageButton").css('display', 'block');
            }else{
                //$('#avatarSlect').css('display', 'none');
            }
        });

    });
</script>
